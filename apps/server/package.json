{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "author": "Cedric Nicoloso <cedric.nicoloso@gmail.com>",
  "scripts": {
    "watch": "tsc -w & tsc-alias -w",
    "dev": "nodemon -r source-map-support/register dist/server.js",
    "build": "rimraf ./dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node dist/server.js",
    "test": "NODE_ENV=test mocha --config .mocharc.unit.js",
    "lint": "npm run prettier",
    "prettier": "prettier src admin-scripts --check",
    "prettier:fix": "prettier src admin-scripts --write",
    "eslint": "eslint src --ext .ts",
    "eslint:fix": "eslint src"
  },
  "dependencies": {
    "@hapi/boom": "9.1.4",
    "fastify": "3.27.1",
    "fastify-helmet": "7.0.1",
    "fastify-plugin": "3.0.1",
    "fastify-sensible": "3.1.2",
    "freely-prisma": "*",
    "module-alias": "2.2.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.11.22",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "chai": "^4.3.6",
    "eslint": "8.8.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "json-schema-to-ts": "1.6.5",
    "mocha": "^9.2.2",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "source-map-support": "0.5.21",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^3.14.1",
    "typescript": "4.6.3"
  }
}
